<h1>trello</h1>
<div class = "columns">
    <% @stickynotes.each do |stickynote| %>
        <div class = "trello"><%= stickynote.body %></div>
    <% end %>
</div>
<form action="/stickynote/index" method = "post">
    <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>">
    <input type="text" name= "body" placeholder="note text"/ >
    <input type="submit">
</form>


